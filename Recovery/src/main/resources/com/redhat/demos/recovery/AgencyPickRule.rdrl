package com.redhat.demos.recovery;

import java.lang.Number;

rule "AgencyPickRule"
	dialect "mvel"
	ruleflow-group "findagents"
	no-loop true
	when
		Order( market == "2" )
		agency : Agency( )
	then
		modify( agency ) {
				setName( "ABC Recovery" ), 
				setRating( 4 )
		}
		System.out.println("fired agencypickrule");
end
