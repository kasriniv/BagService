<scenario>
  <name>Apply Surcharge Test.scenario</name>
  <maxRuleFirings>100000</maxRuleFirings>
  <globals/>
  <fixtures>
    <fact-data>
      <type>Passenger</type>
      <fieldData>
        <field-data>
          <name>state</name>
          <value>AK</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>surcharge</name>
          <value></value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>akPassenger</name>
      <isModify>false</isModify>
    </fact-data>
    <fact-data>
      <type>Passenger</type>
      <fieldData>
        <field-data>
          <name>state</name>
          <value>HI</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>surcharge</name>
          <value></value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>hiPassenger</name>
      <isModify>false</isModify>
    </fact-data>
    <fact-data>
      <type>Passenger</type>
      <fieldData>
        <field-data>
          <name>state</name>
          <value>TX</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>surcharge</name>
          <value></value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>txPassenger</name>
      <isModify>false</isModify>
    </fact-data>
    <org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
      <name>apply-surcharges</name>
    </org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
    <execution-trace/>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>surcharge</fieldName>
          <expected>25.00</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
      </fieldValues>
      <name>akPassenger</name>
      <anonymous>false</anonymous>
    </expect-fact>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>surcharge</fieldName>
          <expected>25.00</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
      </fieldValues>
      <name>hiPassenger</name>
      <anonymous>false</anonymous>
    </expect-fact>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>surcharge</fieldName>
          <expected>0.0</expected>
          <operator>==</operator>
          <nature>0</nature>
        </expect-field>
      </fieldValues>
      <name>txPassenger</name>
      <anonymous>false</anonymous>
    </expect-fact>
  </fixtures>
  <rules/>
  <inclusive>false</inclusive>
  <packageName></packageName>
  <imports>
    <imports>
      <org.drools.workbench.models.datamodel.imports.Import>
        <type>com.fly.traveloptions.bagservice.Passenger</type>
      </org.drools.workbench.models.datamodel.imports.Import>
    </imports>
  </imports>
</scenario>